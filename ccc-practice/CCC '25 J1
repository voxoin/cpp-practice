#include <bits/stdc++.h>

using namespace std;

int main() {
    int n;
    int c;
    int p;
    int ride_space;
    
    std::cin >> n;
    std::cin >> c;
    std::cin >> p;
    
    ride_space = c * p;
    if (ride_space >= n) {
        std::cout << "yes\n";
    }
    else {
        std::cout << "no\n";
    }
    
    
    return 0;
}
